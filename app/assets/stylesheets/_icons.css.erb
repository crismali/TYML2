.icon-like, .icon-delete, .icon-share, .icon-archive {
	font-family: 'ap-icons';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	text-decoration: none;
	-webkit-font-smoothing: antialiased;
}
