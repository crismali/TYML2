@font-face {
	font-family: 'ap-icons';
	src:url('<%= asset_path('ap-icons-bdb3a1186be99cfd89d11a8a084e2265.eot') %>');
	src:url('<%= asset_path('ap-icons-bdb3a1186be99cfd89d11a8a084e2265.eot') %>' + "?#iefix") format('embedded-opentype'),
      url('<%= asset_path('ap-icons-5ab1577ef56bf34dcecad5bad0074c78.woff') %>') format('woff'),
      url('<%= asset_path('ap-icons-bac0b255d1d6f65168d19ca0a2e8c063.ttf') %>')  format('truetype'),
      url('<%= asset_path('ap-icons-665d9221b76222e8a465704686aa529c.svg') %>' + "#MyFont") format('svg');
	font-weight: normal;
	font-style: normal;
}


